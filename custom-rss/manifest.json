{
  "name": "ad-aures/custom-rss",
  "version": "0.1.0",
  "minCastopodVersion": "2.0.0-next.3",
  "description": "Inject custom RSS to your feed's channel and items.",
  "license": "AGPL-3.0-or-later",
  "repository": {
    "url": "https://github.com/ad-aures/castopod-plugins",
    "directory": "custom-rss"
  },
  "authors": [
    {
      "name": "Yassine Doghri",
      "url": "https://yassinedoghri.com/"
    }
  ],
  "hooks": ["rssAfterChannel", "rssAfterItem"],
  "settings": {
    "podcast": {
      "custom-rss": {
        "type": "rss",
        "label": "Custom RSS"
      }
    },
    "episode": {
      "custom-rss": {
        "type": "rss",
        "label": "Custom RSS"
      }
    }
  }
}
